<Window x:Class="Mail.Xamls.MoveToFolder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Mail.Xamls"
        xmlns:lang="clr-namespace:Mail.lang"
        mc:Ignorable="d"
        Title="{x:Static lang:lang.mail_menu_moveto}" Height="250" Width="250" Loaded="MoveToFolder_OnLoaded">
    <DockPanel>
        <Button DockPanel.Dock="Bottom" Height="30" Width="100" PreviewMouseDown="Ok"
                Content="{x:Static lang:lang.mail_menu_moveto}">
        </Button>
        <StackPanel>
            <TextBlock Text="{x:Static lang:lang.folder_select}"></TextBlock>
            <ListBox Name="View">
            </ListBox>
        </StackPanel>
    </DockPanel>
</Window>